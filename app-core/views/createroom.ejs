<% include header %>
<div class="container-fluid " id="main-content">
  <div class="panel-heading"><h3><%= title %></h3></div>
  <div class="row padding-sm">
    <div class="col-sm-4">
      <div class="panel panel-rounded" id="forms">
        <div class="panel-body  ">
          <form method="post" action="/r/create" >
            <fieldset>
              <div class="form-group">
                <label for="exampleInputEmail">Room name</label>
                <input type="text" class="form-control" name="rname" placeholder="Enter a room name">
              </div>
              <button type="submit" class="btn btn-primary">Create <span class="glyphicon glyphicon-arrow-right"></button>
            </fieldset>
          </form>
        </div>
      </div>
    </div>
  </div>
</div>
<% include footer %>